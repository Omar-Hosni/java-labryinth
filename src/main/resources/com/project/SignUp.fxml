<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.SignUpController">
   <children>
      <Label layoutX="257.0" layoutY="39.0" prefHeight="43.0" prefWidth="87.0" text="Sign Up">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="fieldName" layoutX="188.0" layoutY="115.0" prefHeight="25.0" prefWidth="225.0" promptText="Name" />
      <TextField fx:id="fieldConfirmPassword" layoutX="188.0" layoutY="242.0" prefHeight="25.0" prefWidth="225.0" promptText="Confirm Password" />
      <TextField fx:id="fieldPassword" layoutX="188.0" layoutY="200.0" prefHeight="25.0" prefWidth="225.0" promptText="Password" />
      <TextField fx:id="fieldEmail" layoutX="188.0" layoutY="158.0" prefHeight="25.0" prefWidth="225.0" promptText="Email" />
      <Button fx:id="btnSubmit" layoutX="274.0" layoutY="301.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Button fx:id="btnHaveAccount" layoutX="228.0" layoutY="346.0" mnemonicParsing="false" onAction="#goToLogin" text="Already have an account" />
   </children>
</AnchorPane>
